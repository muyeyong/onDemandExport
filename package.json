{
  "name": "ondemandexport",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "keywords": [
    "vite",
    "vue3"
  ],
  "description": "基于vite和vue3按需导入的组件库",
  "scripts": {
    "preview": "vite preview",
    "build": "gulp -f script/build/index.ts",
    "build:lib": "gulp -f packages/components/script/build/index.ts",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@types/lodash": "^4.14.195",
    "lodash": "^4.17.21",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@XY/components": "workspace:^1.0.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/fs-extra": "^11.0.1",
    "@types/gulp": "^4.0.10",
    "@types/gulp-autoprefixer": "^0.0.33",
    "@types/gulp-less": "^0.0.32",
    "@types/node": "^18.14.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "ant-design-vue": "^3.2.17",
    "eslint": "^8.34.0",
    "eslint-plugin-vue": "^9.9.0",
    "fs-extra": "^11.1.1",
    "glob": "^10.2.3",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-less": "^5.0.0",
    "gulp-sass": "^5.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "rollup-plugin-visualizer": "^5.9.2",
    "ts-node": "^10.9.1",
    "typescript": "~4.8.4",
    "typescript-node": "^0.1.3",
    "unplugin-vue-components": "^0.24.1",
    "unplugin-vue-define-options": "^1.3.4",
    "vite": "^4.4.6",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vue-tsc": "^1.2.0"
  }
}
